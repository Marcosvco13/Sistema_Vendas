@model IEnumerable<Sistema_Venda_SI.Model.Models.Produto>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h4>Lista de Produtos</h4>
<div>
    <table>
        <thead>
            <tr>
                <th>
                    Código
                </th>
                <th>
                    Descrição
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.ProCodigo
                    </td>
                    <td>
                        @item.ProDescricao
                    </td>
                </tr>
                
            }
        </tbody>
    </table>
</div>
